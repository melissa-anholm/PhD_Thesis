@article{thooft1971,
title = {Renormalization of massless Yang-Mills fields},
journal = {Nuclear Physics B},
volume = {33},
number = {1},
pages = {173-199},
year = {1971},
issn = {0550-3213},
doi = {https://doi.org/10.1016/0550-3213(71)90395-6},
url = {https://www.sciencedirect.com/science/article/pii/0550321371903956},
author = {G. 't Hooft},
abstract = {The problem of renormalization of gauge fields is studied. It is observed that the use of non-gauge invariant regulator fields is not excluded provided that in the limit of high regulator mass gauge invariance can be restored by means of a finite number of counter-terms in the Lagrangian. Massless Yang-Mills fields can be treated in this manner, and appear to be renormalizable in the usual sense. Consistency of the method is proved for diagrams with non-overlapping divergencies by means of gauge invariant regulators, which however, cannot be interpreted in terms of regulator fields. Assuming consistency the S-matrix is shown to be unitary in any order of the coupling constant. A restriction must be made: no local, parity-changing transformations must be contained in the underlying gauge group. The interactions must conserve parity.}
}

@article{Glashow1959,
title = {The renormalizability of vector meson interactions},
journal = {Nuclear Physics},
volume = {10},
pages = {107-117},
year = {1959},
issn = {0029-5582},
doi = {https://doi.org/10.1016/0029-5582(59)90196-8},
url = {https://www.sciencedirect.com/science/article/pii/0029558259901968},
author = {Sheldon L. Glashow},
abstract = {A new and less restrictive criterion is deduced for the renormalizability of vector meson interactions. As an example, the electromagnetic interaction of a charged vector meson with unit anomalous magnetic moment is shown to be renormalizable. In order to construct a renormalizable theory of the elementary-particle decays in which such a particle plays the role of an intermediary, it is found necessary to assume an intimate connection between decay-inducing and electrodynamic interactions.}
}

